{
  "expo": {
    "name": "UrbanPulse Admin",
    "slug": "urbanpulse-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#14532D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow UrbanPulse Admin to use your location to help you select your admin area."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.urbanpulse.admin",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to help you select your admin area.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location to help you select your admin area."
      }
    },
    "android": {
      "package": "com.urbanpulse.admin",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "cb1143b5-b89e-4071-84fc-fa65794be97c"
      }
    }
  }
}
